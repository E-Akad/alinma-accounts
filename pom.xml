	<groupId>com.alinma.rib</groupId>
    <artifactId>alinma-family-accounts-management</artifactId>
	<version>2.0.9-SNAPSHOT</version>
	<name>alinma-family-accounts-management</name>
	<description>alinma family accounts management microservice</description>
	<packaging>war</packaging>
	<distributionManagement>
	<repository>
	<id>alinmaa-server</id>
	<url>sftp://10.0.31.14/util01/Alinma-backend.git/alinma-family-accounts-management</url>
	</repository>
	</distributionManagement>
	<properties>
        <java.version>1.8</java.version>
        <spring-cloud.version>Greenwich.SR2</spring-cloud.version>
        <alinma.services.version>1.0.5-SNAPSHOT</alinma.services.version>
        <alinma.lov.version>1.0.3</alinma.lov.version>
    </properties>
    
    <scm>
        <connection>scm:git:ssh://git@github.com/E-Akad/alinma-accounts.git</connection>
