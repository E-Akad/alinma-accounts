        <tag>alinma-family-accounts-management-2.0.6</tag>
    </scm>
    

	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter</artifactId>
		</dependency>
		<dependency>
			<groupId>com.alinma.rib</groupId>
			<artifactId>alinma-services</artifactId>
			<version>${alinma.services.version}</version>
		</dependency>
		
		<dependency>
			<groupId>com.alinma.rib</groupId>
			<artifactId>alinma-lov</artifactId>
			<version>${alinma.lov.version}</version>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-devtools</artifactId>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<optional>true</optional>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-web</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework.session</groupId>
